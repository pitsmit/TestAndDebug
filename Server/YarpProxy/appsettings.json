{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "api-route": {
        "ClusterId": "backend-cluster",
        "Match": {
          "Path": "/api/{**catch-all}"
        }
      },
      "swagger-route": {
        "ClusterId": "backend-cluster",
        "Match": {
          "Path": "/api-docs/{**catch-all}"
        }
      },
      "openapi-route": {
        "ClusterId": "backend-cluster",
        "Match": {
          "Path": "/openapi/{**catch-all}"
        }
      },
      "hello-route": {
        "ClusterId": "backend-cluster",
        "Match": {
          "Path": "/hello"
        }
      },
      "login-route": {
        "ClusterId": "backend-cluster",
        "Match": {
          "Path": "/login-redirect"
        }
      },
      "oauth-route": {
        "ClusterId": "backend-cluster",
        "Match": {
          "Path": "/oauth2-redirect.html"
        }
      }
    },
    "Clusters": {
      "backend-cluster": {
        "Destinations": {
          "backend": {
            "Address": "http://localhost:3000/"
          }
        }
      }
    }
  }
}